<%- include('../partials/head') %>
  <%- include('../partials/header') %>

    <body>
      <div id="container">
        <main>
          <jumbotron class="text-center">
            <h1 class="display-4">Weather Tunes</h1>

            <p>Autogenerated music according to the weather of the day</p>

            <% for (var i=0; i < musicUrls.length; i++) { %>
              <div id="audioContainer">
                <p><%=musicUrls[i].audioDate%></p>
                <audio controls>
                  <source src="<%= musicUrls[i].audioLink %>" type="audio/mpeg">
                </audio>
              </div>
              </br>
              <% } %>


          </jumbotron>
        </main>
      </div>
    </body>
    <%- include('../partials/footer') %>